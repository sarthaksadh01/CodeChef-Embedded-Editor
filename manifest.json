{
    "name": "CodeChef ide ",
    "description": "Add IDE to problem page",
    "version": "0.1",
    "permissions": [
        "tabs",
        "http://www.codechef.com/",
        "https://www.codechef.com/",
        "webNavigation"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background" : {
      "scripts": ["background.js"],
      "persistent": true
    },
    "web_accessible_resources": [
        "ide.html",
        "./lib/js/common_functions.js",
        "./lib/js/config.js"
    ],
    "manifest_version": 2
}