{
    "name": "CodeChef ide ",
    "description": "Add IDE to problem page",
    "version": "0.1",
    "permissions": [
        "tabs",
        "<all_urls>",
        "webNavigation"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background" : {
      "scripts": ["background.js"],
      "persistent": true
    },
    "web_accessible_resources": [
        "ide.html"
    ],
    "manifest_version": 2
}